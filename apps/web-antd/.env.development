# 端口号
VITE_PORT=5666

VITE_BASE=/

# 接口地址
VITE_GLOB_API_URL=/api

# Título de la app
VITE_GLOB_APP_TITLE="The Dance Core Admin"

# 是否开启 Nitro Mock服务，true 为开启，false 为关闭
# Desactivar mock para apuntar a NestJS en dev
VITE_NITRO_MOCK=true

# Bypass de login para evaluar la plantilla sin backend (true/false)
VITE_BYPASS_LOGIN=true

# 是否打开 devtools，true 为打开，false 为关闭
VITE_DEVTOOLS=true

# 是否注入全局loading
VITE_INJECT_APP_LOADING=true
